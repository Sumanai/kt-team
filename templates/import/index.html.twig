{% extends 'base.html.twig' %}

{% block title %}
    Import file
{% endblock %}

{% block body %}
    <style>
        .example-wrapper {
            margin: 1em auto;
            max-width: 800px;
            width: 95%;
            font: 18px / 1.5 sans-serif;
        }
    </style>

    <div class="example-wrapper">
        <h1>Import file</h1>
        <p>Choose file in XML format:</p>
        {{ form_start(form) }}
             {{ form_row(form.file) }}

            <button type="submit">Submit</button>
        {{ form_end(form) }}
    </div>
{% endblock %}
